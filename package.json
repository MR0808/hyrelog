{
    "name": "hyrelog",
    "version": "1.0.0",
    "description": "HyreLog - Audit Trail Data API",
    "type": "module",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc -p tsconfig.json",
        "start": "node dist/server.js",
        "lint": "tsc --noEmit",
        "seed": "tsx prisma/seed.ts",
        "worker": "tsx src/workers/index.ts",
        "worker:webhook": "tsx src/workers/webhookWorker.ts",
        "worker:jobs": "tsx src/workers/jobProcessor.ts"
    },
    "keywords": [
        "audit",
        "fastify",
        "prisma"
    ],
    "author": "HyreLog",
    "license": "MIT",
    "engines": {
        "node": ">=22"
    },
    "dependencies": {
        "@aws-sdk/client-glacier": "^3.700.0",
        "@aws-sdk/client-s3": "^3.700.0",
        "@fastify/sensible": "^6.0.3",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/exporter-otlp-http": "^0.26.0",
        "@opentelemetry/instrumentation-fastify": "^0.53.0",
        "@opentelemetry/instrumentation-http": "^0.208.0",
        "@opentelemetry/resources": "^1.27.0",
        "@opentelemetry/sdk-node": "^0.208.0",
        "@opentelemetry/semantic-conventions": "^1.27.0",
        "@prisma/adapter-pg": "^7.0.1",
        "@prisma/client": "^7.0.1",
        "@prisma/instrumentation": "^5.11.0",
        "dotenv": "^16.4.7",
        "fastify": "^5.6.2",
        "fastify-plugin": "^5.0.1",
        "node-cron": "^3.0.3",
        "pg": "^8.16.3",
        "pino": "^9.5.0",
        "zod": "^3.23.8",
        "zod-to-json-schema": "^3.23.3",
        "ajv": "^8.12.0"
    },
    "devDependencies": {
        "@types/node": "^22.9.0",
        "@types/node-cron": "^3.0.11",
        "@types/pg": "^8.15.6",
        "prisma": "^7.0.1",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3"
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    }
}
